{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dart-debug-native",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/dart",
      "args": [
        "${cwd}/dart/lib/iouring_transport.dart"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/dart",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "transport-binding-build"
    },
    {
      "name": "dart-test-debug-native",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/dart",
      "args": [
        "${cwd}/dart/test/test.dart"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/dart",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "transport-binding-build"
    },
    {
      "name": "dart-benchmark-debug-native",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/dart",
      "args": [
        "${cwd}/dart/test/benchmark.dart"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/dart",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "transport-binding-build"
    },
  ]
}